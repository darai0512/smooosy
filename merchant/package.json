  "scripts": {
    "dev": "rm -rf node_modules/.cache/cache-loader && run-p dev:*",
    "dev:api-build": "webpack --config webpack.server.js --watch",
    "dev:@smooosy-build": "tsc --watch -p tsconfig.smooosy.json",
    "dev:api": "./copy_files.sh; nodemon --watch dist/node/src/api/ --delay 2s --exec 'node dist/node/src/api/server.js'",
    "dev:tsc": "tsc --watch -p tsconfig.json",
    "dev:tools": "webpack --config webpack.tools.js --watch",
    "rm": "rm -rf dist/*",
    "sitemap": "node ./dist/node/src/api/script/generateSitemap.js",
    "build-main": "webpack -p --config webpack.build.js",
    "build-api": "tsc -p tsconfig.json; ./copy_files.sh",
    "build-@smooosy/merchant": "tsc -p tsconfig.smooosy.json",
    "build-storybook": "build-storybook -c .storybook -o dist/public/storybook",
    "build": "run-s rm build-main build-api build-storybook build-@smooosy/merchant",
    "build-prod": "run-s rm build-main build-api build-@smooosy/merchant",
    "deploy": "./deploy.sh $NODE_ENV",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "start": "pm2 startOrReload pm2.yml --env $NODE_ENV",
    "stop": "pm2 stop pm2.yml",
    "test:web": "ava -v --fail-fast test/web/request-test.js",
    "test:api": "nyc ava",
    "ci-test:api": "nyc ava --tap | tap-xunit > ~/reports/test/report.xml",
    "ci-test:jest": "jest --coverage",
    "mongo": "node --experimental-repl-await dist/node/src/api/script/repl.js",
    "storybook": "start-storybook -s ./src/static"
  },
